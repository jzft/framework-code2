<?xml version="1.0" encoding="UTF-8" ?>	
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<classPathEntry location="./bin/mysql-connector-java-5.1.34.jar" />
	<!-- <classPathEntry location="./lib/sqljdbc4-4.0.jar" /> -->

	<context id="prod">
		<!-- RowBounds pagination -->
		<plugin type="org.mybatis.generator.plugins.RowBoundsPlugin" />
		<plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin" />
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />

		<commentGenerator>
			<property name="suppressDate" value="true" />
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!-- 数据库连接信息 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/authority?useUnicode=true&amp;characterEncoding=UTF-8"
			userId="lyq" password="123456@aa" />
		<!-- 	<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://119.23.200.51:3306/security?useUnicode=true&amp;characterEncoding=UTF-8"
			userId="jiaxing" password="BQmnav!rXSVXAXP0" /> -->
		<!-- entity跟example位置 -->
		<javaModelGenerator targetPackage="com.ddd.seo.security.pojo"
			targetProject="src/main/java" />
		<!-- xml位置 -->
		<sqlMapGenerator targetPackage="/mybatis/securitymapper"
			targetProject="src/main/resources" />
		<!-- 接口位置 -->
		<javaClientGenerator targetPackage="com.ddd.seo.security.mapper"
			targetProject="src/main/java" type="XMLMAPPER">
			<property name="enableSubPackages" value="false" />
			<property name="rootInterface" value="com.framework.db.BaseMapper" />
		</javaClientGenerator>

		<table tableName="t_permission" domainObjectName="PermissionEntity">
			<generatedKey column="id" sqlStatement="MYSQL" identity="true" />
		</table> 
		<table tableName="t_role" domainObjectName="RoleEntity">
			<generatedKey column="id" sqlStatement="MYSQL" identity="true" />
		</table> 
		<table tableName="t_role_permission" domainObjectName="RolePermissionEntity">
			<generatedKey column="id" sqlStatement="MYSQL" identity="true" />
		</table> 
		<table tableName="t_user_role" domainObjectName="UserRoleEntity">
			<generatedKey column="id" sqlStatement="MYSQL" identity="true" />
		</table> 
		<table tableName="t_user" domainObjectName="UserEntity">
			<generatedKey column="id" sqlStatement="MYSQL" identity="true" />
		</table> 
<!-- 		<table tableName="s_fetch_log" domainObjectName="LogEntity">
			<generatedKey column="id" sqlStatement="MYSQL" identity="true" />
		</table> -->
	</context>
</generatorConfiguration>